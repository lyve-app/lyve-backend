{
  "name": "media-server",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "tsc --project './tsconfig.app.json' --watch & cross-env NODE_ENV=development nodemon --trace-warnings ./dist/index.js --verbose",
    "build": "tsc --project './tsconfig.app.json'",
    "start": "yarn run build && cross-env NODE_ENV=production node --trace-warnings ./dist/index.js",
    "watch": "tsc --project './tsconfig.app.json' --watch",
    "test": "jest --coverage=false --passWithNoTests",
    "test:watch": "jest -i --watchAll --coverage=false --passWithNoTests",
    "coverage": "jest -i --coverage --passWithNoTests"
  },
  "private": true,
  "dependencies": {
    "amqplib": "^0.10.4",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "~4.19.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "http-status": "^1.7.4",
    "joi": "^17.13.1",
    "mediasoup": "^3.14.6",
    "tslib": "^2.3.0",
    "utility-types": "^3.11.0",
    "winston": "^3.13.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "~4.17.13",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.16.9",
    "cross-env": "^7.0.3",
    "eslint": "~8.57.0",
    "jest": "^29.4.1",
    "nodemon": "^3.1.0",
    "prisma": "^5.13.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.4.2"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma"
  }
}
